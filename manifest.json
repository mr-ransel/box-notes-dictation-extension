{
  "manifest_version": 2,

  "name": "One-click Kittens",
  "description": "This extension demonstrates a 'browser action' with kittens.",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": ["https://cloud.app.box.com/services/box_notes/*"],
      "js": ["input.js", "output.js", "jquery.js", "annyang.min.js"]
    },
    {
      "matches": ["https://notes.services.box.com/*"],
      "css": [],
      "js": ["input.js", "output.js", "jquery.js", "annyang.min.js"]
    }
  ]
}
